<idea-plugin>
    <id>net.ntworld.nhat-phan.sentry-integration-community</id>
    <name>Sentry Integration Community</name>
    <vendor>ntworld</vendor>
    <version>1.0.203</version>
    <depends>com.intellij.modules.platform</depends>
    <depends>com.intellij.modules.lang</depends>
    <depends>com.intellij.modules.vcs</depends>

    <extensions defaultExtensionNs="com.intellij">
        <applicationService
                serviceInterface="net.ntworld.sentryIntegrationIdea.serviceProvider.community.ApplicationServiceProviderCommunity"
                serviceImplementation="net.ntworld.sentryIntegrationIdea.serviceProvider.community.ApplicationServiceProviderCommunityImpl"
        />
        <projectService
                serviceInterface="net.ntworld.sentryIntegrationIdea.serviceProvider.community.ProjectServiceProviderCommunity"
                serviceImplementation="net.ntworld.sentryIntegrationIdea.serviceProvider.community.ProjectServiceProviderCommunityImpl"
        />
        <projectConfigurable
                id="sentry-integration-community"
                displayName="Sentry Integration Community"
                instance="net.ntworld.sentryIntegrationIdea.serviceProvider.community.PluginConfigurableCommunity"
        />

        <toolWindow id="Sentry Integration Community"
                    secondary="false"
                    anchor="bottom"
                    icon="/icons/main-tool-window-default.svg"
                    canCloseContents="true"
                    factoryClass="net.ntworld.sentryIntegrationIdea.toolWindow.community.MainToolWindowFactoryCommunity"/>

    </extensions>

    <actions>

    </actions>
</idea-plugin>